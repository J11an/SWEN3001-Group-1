<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jamrockcuisine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JamrockCuisine">
        <activity android:name=".views.SearchResults"></activity>
        <activity android:name=".views.Favorites" />
        <activity android:name=".views.Instructions" />
        <activity android:name=".views.Category" />
        <activity android:name=".views.Ingredients" />
        <activity android:name=".views.BreakfastCategory" />
        <activity android:name=".views.Instructions_AckeeNSaltfish" />
        <activity android:name=".views.Ingredients_AckeeNSaltfish" />
        <activity android:name=".views.LunchCategory" />
        <activity android:name=".views.DinnerCategory" />
        <activity android:name=".views.DesertCategory" />
        <activity
            android:name=".views.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.JamrockCuisine.NoActionBar" />
        <activity android:name=".views.Register" />
        <activity android:name=".views.Login" />
        <activity
            android:name=".views.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>